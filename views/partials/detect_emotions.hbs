<div class="data-ctn">
    <h3>Detect Emotions</h3>
    <div class="description d-flex flex-row align-items-start">
        <img src="/images/icons/info-circle.svg" alt="">
        <p class="subtext ms-2">Count the number of people in the images. Analyze the mood and tone of the images (emotional detection).</p>
    </div>
    <div class="d-flex flex-row justify-content-between mt-3">
        <p style="font-family: bold-font">Total</p>
        <div class="logos-detect flex-row">
            {{#each logo}}
                <img src={{this}} alt="">
            {{/each}}
        </div>
    </div>
    <div class="d-flex flex-row justify-content-between">
        <p class="subtext">Happy</p>
        <p>{{detect_emotions.happy}}</p>
    </div>
    <div class="d-flex flex-row justify-content-between">
        <p class="subtext">Angry</p>
        <p>{{detect_emotions.angry}}</p>
    </div>
    <div class="d-flex flex-row justify-content-between">
        <p class="subtext">Enjoy</p>
        <p>{{detect_emotions.Enjoy}}</p>
    </div>
    <div class="d-flex flex-row justify-content-between">
        <p class="subtext">Relax</p>
        <p>{{detect_emotions.relax}}</p>
    </div>
    <div class="d-flex flex-row justify-content-between">
        <p class="subtext">Neutral</p>
        <p>{{detect_emotions.neutral}}</p>
    </div>
    {{!-- <div class="d-flex flex-row justify-content-between">
        <p class="subtext">Undefined</p>
        <p>{{detect_emotions.undefined}}</p>
    </div> --}}
</div>